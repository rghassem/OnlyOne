	"version": "2.0.0",
	"configurations": [
		{
			"name": "Launch Chrome Webpack",
			"type": "chrome",
			"request": "launch",
			"runtimeArgs": [
				"--incognito"
			],
			"smartStep": true,
			"url": "http://localhost:8080",
			"webRoot": "${workspaceRoot}"
		},
		{
			"name": "Launch Past Tutorial",
			"type": "chrome",
			"request": "launch",
			"runtimeArgs": [
				"--incognito"
			],
			"smartStep": true,
			"url": "http://localhost:8080/?level=10",
			"webRoot": "${workspaceRoot}"
		}
	]
}